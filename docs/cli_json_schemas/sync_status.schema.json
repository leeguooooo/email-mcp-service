{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["success", "scheduler_running", "accounts"],
  "properties": {
    "success": { "type": "boolean" },
    "error": {
    "oneOf": [
      { "type": "string" },
      {
        "type": "object",
        "required": ["code", "message"],
        "properties": {
          "code": { "type": "string" },
          "message": { "type": "string" },
          "detail": {}
        },
        "additionalProperties": true
      }
    ]
  },
    "scheduler_running": { "type": "boolean" },
    "accounts": { "type": "array" },
    "database_size": { "type": "number" }
  },
  "additionalProperties": true
}
