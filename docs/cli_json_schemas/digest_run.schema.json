{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["success", "date", "total_emails", "summary", "dry_run"],
  "properties": {
    "success": { "type": "boolean" },
    "error": {
    "oneOf": [
      { "type": "string" },
      {
        "type": "object",
        "required": ["code", "message"],
        "properties": {
          "code": { "type": "string" },
          "message": { "type": "string" },
          "detail": {}
        },
        "additionalProperties": true
      }
    ]
  },
    "date": { "type": "string" },
    "total_emails": { "type": "number" },
    "summary": { "type": "string" },
    "dry_run": { "type": "boolean" }
  },
  "additionalProperties": true
}
